{
  "name": "github-search",
  "version": "1.0.0",
  "private": true,
  "description": "This TypeScript project provides a basic reimplementation of Github's Search feature using the public API without any authentication steps to be performed by the user.",
  "main": "index.js",
  "scripts": {
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack",
    "build:watch": "npm run build -- --watch",
    "build:prod": "npm run build -- --mode=production",
    "build:watch:prod": "npm run build -- --watch --mode=production",
    "run": "npx http-server ./dist",
    "lint": "npx eslint",
    "lint:fix": "npx eslint --fix",
    "test": "npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevethedev/github-user-search.git"
  },
  "author": {
    "name": "Steven Jimenez",
    "email": "stevenmjimenez@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stevethedev/github-user-search/issues"
  },
  "homepage": "https://github.com/stevethedev/github-user-search#readme",
  "devDependencies": {
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.1",
    "http-server": "^0.12.3",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.4.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4",
    "typescript-plugin-css-modules": "^3.2.0",
    "webpack": "^5.31.0",
    "webpack-cli": "^4.6.0"
